<div class="row">
<% @messages.each do |m| %>

    <div class="col-md-3 rounded" style="background: gainsboro;margin-left:2px">
      <div class="col-md-12">

    <h2><%= m.title %></h2>
    <%= link_to "View Message", message_path(m), class: "btn btn default" %>

      </div>
    </div>
<% end %>

</div>